<!DOCTYPE HTML>
<html>
<head>
    @await Html.PartialAsync("MetatagsPartial")
    @await Html.PartialAsync("CssPartial")
</head>
<body>
    <div id="page-wrapper">
        @await Html.PartialAsync("HeaderPartial")

        <div id="main">
            <div class="container">
                <a>
                    <form class="notNav" method="get" asp-area="" asp-controller="Services" asp-action="Search" enctype="multipart/form-data">
                        <input name="searchString" class="form-control" />
                        <input type="submit" value="Search" class="btn btn-default" />
                    </form>
                </a>
                <div class="col-8 col-12-medium">
                    @RenderBody()
                </div>

            </div>
        </div>
        @await Html.PartialAsync("FooterPartial")
    </div>
    @await Html.PartialAsync("ScriptsPartial")
</body>
</html>